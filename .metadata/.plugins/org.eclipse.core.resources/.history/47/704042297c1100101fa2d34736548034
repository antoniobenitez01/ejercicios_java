package ejercicio2;

import java.time.LocalDate;

public class Llamada 
{
	public enum Tipos {ENTRANTE, SALIENTE};
	private String telefono;
	private LocalDate fecha;
	private LocalTime hora;
	private Tipos tipo;
	
	public Llamada(String telefono, LocalDate fecha, Tipos tipo)
	{
		this.telefono = telefono;
		this.fecha = fecha;
		this.tipo = tipo;
	}
}
