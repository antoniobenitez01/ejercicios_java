package persistencia;

import java.io.*;

public class Main 
{
	public static void main(String[] args) 
	{
		File filecoches = new File("coches.txt");
		if(filecoches.exists()) filecoches.delete();
		try
		{
			filecoches.createNewFile();
		}catch(IOException e)
		{
			System.out.println("Ha ocurrido un problema creando el archivo.");
		}
		
	}
}
