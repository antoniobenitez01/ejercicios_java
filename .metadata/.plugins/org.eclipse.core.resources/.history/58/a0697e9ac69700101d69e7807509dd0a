package configuracion;

import java.util.Properties;
import java.io.*;

public class List {

	public static void main(String[] args) 
	{
		Properties ps = new Properties();
		try {
			ps.load(new FileInputStream("mysqld.cnf"));
			ps.list(System.out);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
